#jw-<%= @gist.url %> .clearfix:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
}
 

#jw-<%= @gist.url %>.jw .pull-left {float: left;}
#jw-<%= @gist.url %>.jw .pull-right {float: right;}
#jw-<%= @gist.url %>.jw {
  font-family: 'helvetica', sans-serif;
  position: relative;
  overflow-x: hidden;
  background: #232323;
  border-radius: 4px;
}
#jw-<%= @gist.url %>.jw .jw-body {
  overflow-x: scroll;
  overflow-y:auto;
  position: relative;
  z-index: 1;
  max-height: 300px
}
#jw-<%= @gist.url %>.jw .jw-canvas {position: absolute;left: 50px;top: 10px;z-index: -1;}
#jw-<%= @gist.url %>.jw .jw-canvas img {
  max-width: none;
}

#jw-<%= @gist.url %>.jw .jw-footer {
  background-color: #2ecc71;
  padding: 10px;
  letter-spacing: 0px;
  word-spacing: -2px;
}
#jw-<%= @gist.url %>.jw .jw-footer a {
  color: #fff;
  text-decoration: none;
  font-size: 14px;
  vertical-align: middle;
  display: inline-block;
}
#jw-<%= @gist.url %>.jw .jw-footer a.hosted {padding: 3px 10px;}
#jw-<%= @gist.url %>.jw .jw-footer a.hosted .fa-heart {color: #c0392b;}
#jw-<%= @gist.url %>.jw .jw-footer a {
  vertical-align:bottom;
  white-space: nowrap;
  border: none;
}
#jw-<%= @gist.url %>.jw .jw-footer .jw-meta a img {
  margin-left: 3px;
  margin-right: 10px;
  border-radius: 50px;
}
#jw-<%= @gist.url %>.jw .jw-footer .jw-meta .jw-title {
  padding: 3px 0px;
  display: inline-block;
}
#jw-<%= @gist.url %>.jw pre {
  display: block;
  padding: 9.5px;
  margin: 0 0 15px 0;
  font-size: 12px;
  line-height: 1.4;
  word-break: break-all;
  word-wrap: break-word;
  font-family: monospace;
  color: #fff;
}

/* CodeRay syntax highlighting */
#jw-<%= @gist.url %> .CodeRay {margin: 0px;padding: 0px;}
#jw-<%= @gist.url %> .CodeRay pre {background: transparent;border: none;}
#jw-<%= @gist.url %> .CodeRay .code {
  padding-left: 5px;
  position: absolute;
  top: 0;
  left: 32px;

}
#jw-<%= @gist.url %> .CodeRay .line-numbers {
  margin: 0px 0px -2px -2px;
  display: inline-table;
}
#jw-<%= @gist.url %> .CodeRay .line-numbers pre {
  padding: 8px 12px;
  border-right: 1px solid rgba(255, 255, 255, 0.12);
  border-radius: 0px;
  text-align: center;
}
#jw-<%= @gist.url %>.CodeRay .line-numbers pre a {text-decoration: none;color: #fff;}

/* JustWatch default theme */
#jw-<%= @gist.url %>.jw-default {background-color: #F5F5F5;border: none !important;overflow-y: hidden;}
#jw-<%= @gist.url %>.jw-default pre {color: #333; padding: 8px;}
#jw-<%= @gist.url %>.jw-default .line-numbers {background-color: #EBEBEB;margin: -2px 0px -2px -2px; display: inline-table;}
#jw-<%= @gist.url %>.jw-default .line-numbers pre a {color: #888888;}
#jw-<%= @gist.url %>.jw-default .jw-footer {background-color: #6CCAC9;position: relative;z-index: 2;}


